{
  "name": "fastify-autosecurity",
  "version": "0.1.1",
  "description": "fastest way to map directories to URLs in fastify",
  "main": "dist",
  "prettier": "@giovannicardamone/prettier-config",
  "eslintConfig": {
    "extends": "@giovannicardamone/prettier-config"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint .",
    "test": "tap --ts --node-arg=--require=ts-node/register tests/**/*.test.ts",
    "test:coverage": "tap --ts --node-arg=--require=ts-node/register --cov --coverage-report=lcovonly tests/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GiovanniCardamone/fastify-autosecurity.git"
  },
  "author": "Giovanni Cardamone <g.cardamone2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GiovanniCardamone/fastify-autosecurity/issues"
  },
  "homepage": "https://github.com/GiovanniCardamone/fastify-autosecurity#readme",
  "devDependencies": {
    "@giovannicardamone/eslint-config": "^0.4.2",
    "@giovannicardamone/prettier-config": "^0.1.3",
    "@types/glob": "^7.1.4",
    "@types/node": "^16.4.13",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fastify": "3.20.2",
    "tap": "^15.0.9",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "glob-promise": "^4.2.0"
  },
  "peerDependencies": {
    "fastify": "^3.20.2",
    "fastify-swagger": "^4.9.0"
  }
}
